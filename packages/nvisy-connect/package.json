{
	"name": "@nvisy/connect",
	"version": "0.1.0",
	"description": "Connectors for object storage, relational databases, and vector stores",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"imports": {
		"#core/*.js": "./src/core/*.ts",
		"#object/*.js": "./src/object/*.ts",
		"#relational/*.js": "./src/relational/*.ts",
		"#vector/*.js": "./src/vector/*.ts"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@langchain/community": "^1.1.9",
		"@langchain/core": "^1.1.17",
		"@langchain/pinecone": "^1.0.1",
		"@langchain/qdrant": "^1.0.1",
		"@langchain/weaviate": "^1.0.1",
		"@nvisy/core": "*",
		"effect": "^3.19.14"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
