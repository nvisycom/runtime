{
	"name": "@nvisy/runtime",
	"version": "0.1.0",
	"description": "Graph definition, DAG compilation, and execution engine for the Nvisy platform",
	"type": "module",
	"exports": {
		".": {
			"source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
