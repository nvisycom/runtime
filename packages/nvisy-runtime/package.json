		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@logtape/logtape": "^2.0.2",
		"@nvisy/core": "*",
		"graphology": "^0.26.0",
		"graphology-dag": "^0.4.1",
		"graphology-types": "^0.24.7",
		"effection": "^4.0.0",
		"zod": "^4.3.0"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
