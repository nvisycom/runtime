{
	"name": "@nvisy/process",
	"version": "0.1.0",
	"description": "Document processing pipeline: partition, chunk, embed, enrich, and extract",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"clean": "rimraf dist",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@nvisy/core": "*",
		"@langchain/anthropic": "^0.3.0",
		"@langchain/cohere": "^0.3.0",
		"@langchain/core": "^0.3.0",
		"@langchain/google-genai": "^0.2.0",
		"@langchain/openai": "^0.4.0"
	},
	"imports": {
		"#base/*.js": "./src/base/*.ts",
		"#completion/*.js": "./src/completion/*.ts",
		"#embedding/*.js": "./src/embedding/*.ts",
		"#transform/*.js": "./src/transform/*.ts",
		"#transform/config/*.js": "./src/transform/config/*.ts",
		"#transform/chunk/*.js": "./src/transform/chunk/*.ts",
		"#transform/convert/*.js": "./src/transform/convert/*.ts",
		"#transform/derive/*.js": "./src/transform/derive/*.ts",
		"#transform/enrich/*.js": "./src/transform/enrich/*.ts",
		"#transform/extract/*.js": "./src/transform/extract/*.ts",
		"#transform/metadata/*.js": "./src/transform/metadata/*.ts",
		"#transform/partition/*.js": "./src/transform/partition/*.ts",
		"#transform/route/*.js": "./src/transform/route/*.ts"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
