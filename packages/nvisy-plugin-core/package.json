{
	"name": "@nvisy/plugin-core",
	"version": "0.1.0",
	"description": "Core plugin with built-in chunk and partition actions for the Nvisy platform",
	"type": "module",
	"exports": {
		".": {
			"source": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"build:watch": "tsup --watch",
		"clean": "rimraf dist",
		"typecheck": "tsc -b"
	},
	"dependencies": {
		"@logtape/logtape": "^2.0.2",
		"@nvisy/core": "*",
		"zod": "^4.3.6"
	},
	"engines": {
		"node": ">=22.0.0"
	}
}
